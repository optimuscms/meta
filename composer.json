{
    "name": "optix/meta",
    "description": "Store meta data for the web against your Laravel Models",
    "type": "library",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8"
    },
  "autoload": {
    "psr-4": {
      "Optix\\Meta\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Optix\\Meta\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Optix\\Meta\\MetaServiceProvider"
      ]
    }
  }
}
